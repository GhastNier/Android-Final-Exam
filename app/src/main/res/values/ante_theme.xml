<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.MaterialComponents.DayNight.Dialog.bg" parent="Theme.AppCompat.DayNight.Dialog">
        <item name="android:background">@color/material_dynamic_tertiary80</item>
        <item name="android:buttonBarNegativeButtonStyle">@style/NegativeButtonStyle</item>
        <item name="android:buttonBarPositiveButtonStyle">@style/PositiveButtonStyle</item>
    </style>
<!--Positive-->
    <style name="PositiveButtonStyle" parent="ThemeOverlay.AppCompat.Dialog.Alert">
        app:icon="@drawable/shop_add"
        style="@style/Widget.MaterialComponents.Button.Icon"
    </style>
    <!--Negative-->
    <style name="NegativeButtonStyle" parent="ThemeOverlay.AppCompat.Dialog.Alert">
        app:icon="@drawable/ic_add_24dp"
        style="@style/Widget.MaterialComponents.Button.Icon"
    </style>
</resources>